@using BeCoreApp.Extensions;
<!doctype html>
<html lang="en" dir="ltr">
<head>
    <!-- Meta data -->
    <meta charset="UTF-8">
    <meta http-equiv="x-ua-compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta content="Dashlot - Bootstrap Responsive Admin panel Dashboard Template Ui Kit & Premium Dashboard Design Modern Flat HTML Template. This Template Includes 100 HTML Pages & 40+ Plugins. No Need to do hard work for this template customization." name="description">
    <meta content="Spruko Technologies Private Limited" name="author">
    <meta name="keywords" content="admin,dashboard,dashboard template,bootstrap dashboard template,bootstrap admin template,admin panel template,admin dashboard template,admin template,admin dashboard,bootstrap 4 admin template,bootstrap dashboard,bootstrap simple dashboard,bootstrap 4 dashboard template,modern admin,bootstrap admin panel,best admin templates,bootstrap admin dashboard template,simple admin panel template,html admin template,simple bootstrap admin template,bootstrap admin dashboard,adminlte template,html dashboard template,responsive admin template,admin dashboard bootstrap 4,admin panel design,bootstrap admin,web dashboard,user dashboard,bootstrap 4 dashboard,bootstrap admin panel template,simple dashboard html template">

    <!-- Favicon-->
    <link rel="icon" href="~/admin-side/dashlot-admin/HTML/assets/images/brand/favicon.png" type="image/x-icon" />

    <!-- Title -->
    <title>Dashlot – Bootstrap 4 Multipurpose  HTML, CSS, Jquery  Admin Dashboard UIKit HTML Template Mega Pack</title>

    <!-- Bootstrap css -->
    <link href="~/admin-side/dashlot-admin/HTML/assets/plugins/bootstrap-4.1.3/css/bootstrap.min.css" rel="stylesheet" />

    <!-- Style css -->
    <link href="~/admin-side/dashlot-admin/HTML/assets/css/style.css" rel="stylesheet" />
    <link href="~/admin-side/dashlot-admin/HTML/assets/css/dark-boxed.css" rel="stylesheet" />

    <!-- Default css -->
    <link href="~/admin-side/dashlot-admin/HTML/assets/css/default.css" rel="stylesheet">

    <!-- Sidemenu css-->
    <link rel="stylesheet" href="~/admin-side/dashlot-admin/HTML/assets/plugins/sidemenu/icon-sidemenu.css">

    <!-- Bootstrap-daterangepicker css -->
    <link rel="stylesheet" href="~/admin-side/dashlot-admin/HTML/assets/plugins/bootstrap-daterangepicker/daterangepicker.css">

    <!-- Bootstrap-datepicker css -->
    <link rel="stylesheet" href="~/admin-side/dashlot-admin/HTML/assets/plugins/bootstrap-datepicker/bootstrap-datepicker.css">

    <!-- Custom scroll bar css -->
    <link href="~/admin-side/dashlot-admin/HTML/assets/plugins/scroll-bar/jquery.mCustomScrollbar.css" rel="stylesheet" />

    <!-- Sidemenu-repsonsive-tabs  css -->
    <link href="~/admin-side/dashlot-admin/HTML/assets/plugins/sidemenu-responsive-tabs/css/sidemenu-responsive-tabs.css" rel="stylesheet">

    <!-- P-scroll css -->
    <link href="~/admin-side/dashlot-admin/HTML/assets/plugins/p-scroll/p-scroll.css" rel="stylesheet" type="text/css">

    <!-- Nice-select css  -->
    <link href="~/admin-side/dashlot-admin/HTML/assets/plugins/jquery-nice-select/css/nice-select.css" rel="stylesheet" />

    <!-- Select2 Plugin -->
    <link href="~/admin-side/dashlot-admin/HTML/assets/plugins/select2/select2.min.css" rel="stylesheet" />

    <!-- News-Ticker css-->
    <link href="~/admin-side/dashlot-admin/HTML/assets/plugins/newsticker/jquery.jConveyorTicker.css" rel="stylesheet" />

    <!-- Font icons css-->
    <link href="~/admin-side/dashlot-admin/HTML/assets/css/icons.css" rel="stylesheet">

    <!-- Rightsidebar css -->
    <link href="~/admin-side/dashlot-admin/HTML/assets/plugins/sidebar/sidebar.css" rel="stylesheet">

    <!-- Color-palette css-->
    <link rel="stylesheet" href="~/admin-side/dashlot-admin/HTML/assets/css/skins.css" />

    @*<link rel="stylesheet" href="~/admin-side/dashlot-admin/HTML/assets/css/style-admin.css" />*@

    @RenderSection("Styles", required: false)
</head>
<body class="app sidebar-mini dark-mode">

    <!-- Loader -->
    <div id="loading">
        <img src="~/admin-side/dashlot-admin/HTML/assets/images/other/loader-dark.svg" class="loader-img" alt="Loader">
    </div>

    <!-- PAGE -->
    <div class="page">
        <div class="page-main">

            <!-- Top-header opened -->
            @await Html.PartialAsync("_HeaderPartial")
            <!-- Top-header closed -->
            <!-- Sidebar menu-->
            @await Html.PartialAsync("_LeftSidebarPartial")
            <!-- Sidemenu closed -->
            <!-- App-content opened -->
            @RenderBody()
            <!-- App-content closed -->
        </div>

        <!-- Right-sidebar-->
        @await Html.PartialAsync("_RightSidebarPartial")
        <!-- Right-sidebar-closed -->
        <!-- Footer opened -->
        @await Html.PartialAsync("_FooterPartial")
        <!-- Footer closed -->
    </div>

    <!-- Back-to-top -->
    <a href="#top" id="back-to-top"><i class="fa fa-angle-double-up"></i></a>

    <!-- Jquery-scripts -->
    <script src="~/admin-side/dashlot-admin/HTML/assets/js/vendors/jquery-3.2.1.min.js"></script>

    <!-- Moment js-->
    <script src="~/admin-side/dashlot-admin/HTML/assets/plugins/moment/moment.min.js"></script>

    <!-- Bootstrap-scripts js -->
    <script src="~/admin-side/dashlot-admin/HTML/assets/js/vendors/bootstrap.bundle.min.js"></script>

    <!-- Sparkline JS-->
    <script src="~/admin-side/dashlot-admin/HTML/assets/js/vendors/jquery.sparkline.min.js"></script>

    <!-- Bootstrap-daterangepicker js -->
    <script src="~/admin-side/dashlot-admin/HTML/assets/plugins/bootstrap-daterangepicker/daterangepicker.js"></script>

    <!-- Bootstrap-datepicker js -->
    <script src="~/admin-side/dashlot-admin/HTML/assets/plugins/bootstrap-datepicker/bootstrap-datepicker.js"></script>

    <!-- Chart-circle js -->
    <script src="~/admin-side/dashlot-admin/HTML/assets/js/vendors/circle-progress.min.js"></script>

    <!-- Rating-star js -->
    <script src="~/admin-side/dashlot-admin/HTML/assets/plugins/rating/jquery.rating-stars.js"></script>

    <!-- Custom scroll bar js-->
    <script src="~/admin-side/dashlot-admin/HTML/assets/plugins/scroll-bar/jquery.mCustomScrollbar.concat.min.js"></script>

    <!-- P-scroll js -->
    <script src="~/admin-side/dashlot-admin/HTML/assets/plugins/p-scroll/p-scroll.js"></script>
    <script src="~/admin-side/dashlot-admin/HTML/assets/plugins/p-scroll/p-scroll-1.js"></script>

    <!-- Sidemenu js-->
    <script src="~/admin-side/dashlot-admin/HTML/assets/plugins/sidemenu/icon-sidemenu.js"></script>

    <!-- Sidemenu-respoansive-tabs js -->
    <script src="~/admin-side/dashlot-admin/HTML/assets/plugins/sidemenu-responsive-tabs/js/sidemenu-responsive-tabs.js"></script>

    <!-- Leftmenu js -->
    <script src="~/admin-side/dashlot-admin/HTML/assets/js/left-menu.js"></script>

    <!--Newsticker js-->
    <script src="~/admin-side/dashlot-admin/HTML/assets/plugins/newsticker/jquery.jConveyorTicker.js"></script>
    <script src="~/admin-side/dashlot-admin/HTML/assets/js/newsticker.js"></script>

    <!-- Chart js-->
    <script src="~/admin-side/dashlot-admin/HTML/assets/plugins/chart/chart.min.js"></script>

    <!-- Nice-select js-->
    <script src="~/admin-side/dashlot-admin/HTML/assets/plugins/jquery-nice-select/js/jquery.nice-select.js"></script>
    <script src="~/admin-side/dashlot-admin/HTML/assets/plugins/jquery-nice-select/js/nice-select.js"></script>

    <!-- Apexchart js-->
    <script src="~/admin-side/dashlot-admin/HTML/assets/js/apexcharts.js"></script>

    <!-- Rightsidebar js -->
    <script src="~/admin-side/dashlot-admin/HTML/assets/plugins/sidebar/sidebar.js"></script>

    <!-- Index js-->
    <script src="~/admin-side/dashlot-admin/HTML/assets/js/index2-dark.js"></script>

    <!-- Custom js-->
    <script src="~/admin-side/dashlot-admin/HTML/assets/js/custom-dark.js"></script>


    <!-- twbs-pagination -->
    <script src="~/lib/twbs-pagination/jquery.twbsPagination.js"></script>

    <script src="~/admin-app/shared/be.js"></script>

    @RenderSection("Scripts", required: false)

</body>
</html>